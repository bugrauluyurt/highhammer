{
  "name": "highhammer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "docker:pre": "node ./tools/dev-scripts/docker-pre.js",
    "docker:client-app": "npm run docker:pre client-app && npx nx run client-app:serve --host=0.0.0.0",
    "docker:api-core": "npm run docker:pre api-core && npx nx run api-core:serve --host=0.0.0.0",
    "docker:qa:up": "cross-env NX_APP_ENV=qa docker compose up",
    "docker:dev:up": "cross-env NX_APP_ENV=dev docker compose up",
    "docker:local:host:up": "cross-env NX_APP_ENV=host docker compose -f ./docker-compose.local.yaml up",
    "docker:local:qa:up": "cross-env NX_APP_ENV=qa docker compose -f ./docker-compose.local.yaml up",
    "docker:local:dev:up": "cross-env NX_APP_ENV=dev docker compose -f ./docker-compose.local.yaml up",
    "docker:up": "npm run docker:local:dev:up",
    "docker:down": "docker compose down",
    "docker:build": "docker compose build",
    "check": "npm run check:node-modules && npm run check:dependencies",
    "check:node-modules": "node ./tools/dev-scripts/cmd-check-node-modules.js",
    "check:dependencies": "node ./tools/dev-scripts/cmd-check-dependencies.js",
    "setup": "npm run check:dependencies && npm install",
    "start": "npm run start:local:host",
    "start:qa": "npm run check && npm run docker:qa:up",
    "start:dev": "npm run check && npm run docker:dev:up",
    "start:local:host": "npm run check && npm run docker:local:host:up",
    "start:local:qa": "npm run check && npm run docker:local:qa:up",
    "start:local:dev": "npm run check && npm run docker:local:dev:up",
    "stop": "npm run docker:down",
    "stop:prune": "npm run docker:down && node ./tools/dev-scripts/cmd-prune.js",
    "nx:start": "npx nx serve",
    "nx:build": "npx nx build",
    "nx:test": "npx nx test"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.5.10",
    "@nrwl/cypress": "14.5.10",
    "@nrwl/eslint-plugin-nx": "14.5.10",
    "@nrwl/jest": "14.5.10",
    "@nrwl/linter": "14.5.10",
    "@nrwl/nest": "14.5.10",
    "@nrwl/node": "14.5.10",
    "@nrwl/react": "14.5.10",
    "@nrwl/web": "14.5.10",
    "@nrwl/workspace": "14.5.10",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-is": "17.0.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "babel-plugin-styled-components": "1.10.7",
    "cross-env": "^7.0.3",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "nx": "14.5.10",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2",
    "yargs": "^17.5.1"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "async": "^3.2.4",
    "bluebird": "^3.7.2",
    "core-js": "^3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-router-dom": "6.3.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "styled-components": "5.3.5",
    "tslib": "^2.3.0"
  }
}
