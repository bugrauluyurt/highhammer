{
  "name": "highhammer",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "docker:client-app": "pnpm nx run client-app:serve --host=0.0.0.0",
    "docker:api-customer-service": "pnpm nx run api-customer-service:serve --host=0.0.0.0",
    "docker:client:qa:up": "cross-env NX_APP_ENV=qa docker compose -f ./docker-compose.client-app.yaml up",
    "docker:client:dev:up": "cross-env NX_APP_ENV=dev docker compose -f ./docker-compose.client-app.yaml up",
    "docker:workspace:host:up": "cross-env NX_APP_ENV=host docker compose -f ./docker-compose.workspace.yaml up",
    "docker:workspace:qa:up": "cross-env NX_APP_ENV=qa docker compose -f ./docker-compose.workspace.yaml up",
    "docker:workspace:dev:up": "cross-env NX_APP_ENV=dev docker compose -f ./docker-compose.workspace.yaml up",
    "docker:up": "pnpm docker:workspace:host:up",
    "docker:down": "docker compose -f ./docker-compose.workspace.yaml down",
    "docker:build": "docker compose -f ./docker-compose.workspace.yaml build",
    "check": "pnpm check:node-modules && npm run check:dependencies",
    "check:node-modules": "node ./tools/dev-scripts/cmd-check-node-modules.js",
    "check:dependencies": "node ./tools/dev-scripts/cmd-check-dependencies.js",
    "clean:node-modules": "bash ./tools/dev-scripts/cmd-clean-node-modules.sh",
    "start": "pnpm start:workspace:host",
    "start:client:qa": "pnpm check && pnpm docker:client:qa:up",
    "start:client:dev": "pnpm check && pnpm docker:client:dev:up",
    "start:workspace:host": "pnpm check && pnpm docker:workspace:host:up",
    "start:workspace:qa": "pnpm check && pnpm docker:workspace:qa:up",
    "start:workspace:dev": "pnpm check && pnpm docker:workspace:dev:up",
    "stop": "pnpm docker:down",
    "stop:prune": "pnpm docker:down --rmi=all --volumes --remove-orphans && node ./tools/dev-scripts/cmd-prune.js",
    "nx": "nx",
    "nx:start": "pnpm check:dependencies --only=node && pnpm nx serve",
    "nx:build": "pnpm check:dependencies --only=node && pnpm nx run-many --target=build --all",
    "nx:test": "pnpm check:dependencies --only=node && pnpm nx run-many --target=test --all",
    "nx:test:affected": "pnpm check:dependencies --only=node && pnpm nx affected --target=test",
    "nx:migrate:next": "pnpm check:dependencies --only=node && pnpm nx migrate next",
    "nx:migrate:run": "pnpm check:dependencies --only=node && pnpm install --no-frozen-lockfile && pnpm nx migrate --run-migrations"
  },
  "devDependencies": {
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.1",
    "@nrwl/nx-cloud": "16.0.5",
    "@nrwl/webpack": "^16.2.1",
    "@nx/cypress": "16.3.0-beta.4",
    "@nx/eslint-plugin": "16.3.0-beta.4",
    "@nx/jest": "16.3.0-beta.4",
    "@nx/js": "16.3.0-beta.4",
    "@nx/linter": "16.3.0-beta.4",
    "@nx/nest": "16.3.0-beta.4",
    "@nx/node": "16.3.0-beta.4",
    "@nx/react": "16.3.0-beta.4",
    "@nx/web": "16.3.0-beta.4",
    "@nx/workspace": "16.3.0-beta.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@semantic-release/exec": "^6.0.3",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.4.4",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-is": "17.0.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "autoprefixer": "10.4.13",
    "babel-jest": "29.4.3",
    "babel-plugin-styled-components": "1.10.7",
    "calver": "^22.8.4",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "http-server": "^14.1.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "nx": "16.3.0-beta.4",
    "postcss": "8.4.21",
    "postcss-import": "15.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.14.1",
    "react-refresh": "^0.14.0",
    "semantic-release": "^19.0.5",
    "tailwindcss": "3.2.4",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "url-loader": "^4.1.1",
    "yargs": "^17.5.1"
  }
}
