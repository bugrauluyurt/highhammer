{
  "name": "highhammer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "docker:pre": "node ./tools/dev-scripts/docker-pre.js",
    "docker:client-app": "npm run docker:pre client-app && npx nx run client-app:serve --host=0.0.0.0",
    "docker:api-core": "npm run docker:pre api-core && npx nx run api-core:serve --host=0.0.0.0",
    "docker:client:qa:up": "cross-env NX_APP_ENV=qa docker compose -f ./docker-compose.client-app.yaml up",
    "docker:client:dev:up": "cross-env NX_APP_ENV=dev docker compose -f ./docker-compose.client-app.yaml up",
    "docker:workspace:host:up": "cross-env NX_APP_ENV=host docker compose -f ./docker-compose.workspace.yaml up",
    "docker:workspace:qa:up": "cross-env NX_APP_ENV=qa docker compose -f ./docker-compose.workspace.yaml up",
    "docker:workspace:dev:up": "cross-env NX_APP_ENV=dev docker compose -f ./docker-compose.workspace.yaml up",
    "docker:up": "npm run docker:workspace:dev:up",
    "docker:down": "docker compose -f ./docker-compose.workspace.yaml down",
    "docker:build": "docker compose -f ./docker-compose.workspace.yaml build",
    "check": "npm run check:node-modules && npm run check:dependencies",
    "check:node-modules": "node ./tools/dev-scripts/cmd-check-node-modules.js",
    "check:dependencies": "node ./tools/dev-scripts/cmd-check-dependencies.js",
    "setup": "npm run check:dependencies && npm install",
    "start": "npm run start:workspace:host",
    "start:client:qa": "npm run check && npm run docker:client:qa:up",
    "start:client:dev": "npm run check && npm run docker:client:dev:up",
    "start:workspace:host": "npm run check && npm run docker:workspace:host:up",
    "start:workspace:qa": "npm run check && npm run docker:workspace:qa:up",
    "start:workspace:dev": "npm run check && npm run docker:workspace:dev:up",
    "stop": "npm run docker:down",
    "stop:prune": "npm run docker:down -- --rmi=all --volumes --remove-orphans && node ./tools/dev-scripts/cmd-prune.js",
    "nx:start": "npm run check:dependencies -- --only=node && npx nx serve",
    "nx:build": "npm run check:dependencies -- --only=node && npx nx build",
    "nx:test": "npm run check:dependencies -- --only=node && npx nx test",
    "nx:migrate:latest": "npm run check:dependencies -- --only=node && npx nx migrate latest",
    "nx:migrate:run": "npm run check:dependencies -- --only=node && nx migrate --run-migrations"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.7.5",
    "@nrwl/cypress": "14.7.5",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/jest": "14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/nest": "14.7.5",
    "@nrwl/node": "14.7.5",
    "@nrwl/react": "14.7.5",
    "@nrwl/web": "14.7.5",
    "@nrwl/workspace": "14.7.5",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-is": "17.0.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "autoprefixer": "10.4.8",
    "babel-jest": "28.1.3",
    "babel-plugin-styled-components": "1.10.7",
    "cross-env": "^7.0.3",
    "cypress": "^10.7.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "28.1.3",
    "nx": "14.7.5",
    "postcss": "8.4.16",
    "postcss-import": "15.0.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.1.8",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2",
    "yargs": "^17.5.1"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "async": "^3.2.4",
    "bluebird": "^3.7.2",
    "core-js": "^3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-router-dom": "6.3.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "styled-components": "5.3.5",
    "tslib": "^2.3.0"
  }
}

